/* Wifi Table */
QTableWidget {
    background-color: rgba(255, 255, 255, 0.04);
    color: rgba(255, 255, 255, 0.9);
    font-size: 14px;
    font-weight: 700;
    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
    border: none;
    padding: 4px;
    border-radius: 10px;
    border: 4px solid transparent;
}

QTableWidget::item {
    padding: 10px;
    border-bottom: 1px solid #444;
}
