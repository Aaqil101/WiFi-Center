/* Wifi Table Windows 10 */
QTableWidget {
    background-color: rgb(31, 39, 56);
    color: rgb(255, 255, 255);
    font-size: 14px;
    font-weight: 700;
    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
    border: none;
    padding: 4px;
    border-radius: 10px;
    border: 4px solid rgb(57, 72, 104);
}

QTableWidget::item {
    padding: 10px;
    border-radius: 10px;
    border: 1px solid snow;
}
