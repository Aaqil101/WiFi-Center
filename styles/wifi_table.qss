/* Wifi Table */
QTableWidget {
    border: 4px solid #3c3c3c;
    border-radius: 10px;
    background-color: #222;
    color: white;
    font-weight: 700;
    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
    font-size: 14px;
}

QTableWidget::item {
    padding: 10px;
    border-bottom: 1px solid #444;
}
