/* log display */
QTextEdit {
    background-color: rgba(25, 44, 66, 0.55);
    color: rgba(255, 255, 255, 0.91);
    font-size: 14px;
    font-weight: 700;
    font-style: normal;
    font-family: "JetBrainsMonoNL Nerd Font Propo", Arial, sans-serif;
    padding: 4px;
    border: none;
    border-radius: 4px;
    border: 2px solid transparent;
}

QTextEdit::-webkit-scrollbar {
    width: 2px;
    height: 2px;
    background: transparent;
}
